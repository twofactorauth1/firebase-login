html(class="no-js", lang="en")
    // <![endif]
    head
        block head
            //- override this to include a different head section (e.g. requirehead.jade)
            block headattributes
                include standardhead
            title=title

            if account && account.orgId == 1
                link(rel="shortcut icon", href='https://s3.amazonaws.com/indigenous-digital-assets/account_1301/rvlvr-icon_1489761269532.png')
            else if account && account.orgId == 5
                link(rel="shortcut icon", href='https://s3.amazonaws.com/indigenous-digital-assets/account_1377/favicon_1508748570313.png')
            else if account && account.orgId == 6
                link(rel="shortcut icon", href='https://s3.amazonaws.com/indigenous-digital-assets/account_1395/favicon_1510866844738.png')
            else
                link(rel="shortcut icon", href='https://s3.amazonaws.com/indigenous-account-websites/acct_6/favicon.ico')

            // Bootstrap
            link(rel='stylesheet', href='/js/libs/bootstrap/dist/css/bootstrap.min.css')
            // Font Awesome
            link(rel='stylesheet', href='/js/libs/font-awesome/css/font-awesome.min.css')
            // CSS
            link(rel='stylesheet', href='/admin/assets/css/styles.css')
            // Theme
            link(rel='stylesheet', data-ng-href='/admin/assets/css/theme.css')
            link(rel='stylesheet', href='/css/styles.css')

    body
        block content

    include _serverprops

