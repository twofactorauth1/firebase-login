{
    "template-engine": "handlebars",
    "supports-menu-lists": "false",

    "theme-id": "fitness-coach",
    "theme-name": "fitness-coach",
    "theme-description": "The fitness-coach theme for INDIGENOUS SOFTWARE, INC.",
    "tags": ["default", "awesome", "basic"],

    "linkLists": [
        {
            "name": "Head2 Menu",
            "handle": "head-menu",
            "links": [
                {
                    "label": "Home",
                    "type": "link",
                    "linkTo": {
                        "type": "home",
                        "data": "index"
                    }
                },
                {
                    "label": "Blog",
                    "type": "link",
                    "linkTo": {
                        "type": "page",
                        "data": "blog"
                    }
                },
                {
                    "label": "Cart",
                    "type": "link"
                }
            ]
        }
    ],

    "pages": [
        {
            "handle": "index",
            "title": "Home",
            "components": [
                "masthead",
                "signup-form",
                "products",
                "social"
            ]
        },

        {
            "handle": "blog",
            "title": "Blog",
            "components": [
                "blog"
            ]
        },
        {
            "handle": "single-post",
            "title": "Single Post",
            "components": [
                "single-post"
            ]
        }
    ],

    "excluded-components": [
    ]
}