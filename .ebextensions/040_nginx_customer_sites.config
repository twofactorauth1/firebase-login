---
files:
  /etc/nginx/conf.d/00_indigeweb_christmas_com.conf:
    content: |
        server {
          listen 8080;
          server_name .christmasintierrasanta.com;
          location / {
            set $hostheader $host;
            if ( $host !~* "indigenous.io" ){
              set $hostheader christmasintierrasanta.indigenous.io;
            }
            proxy_pass $scheme://nodejs;
            proxy_set_header  Host  christmasintierrasanta.indigenous.io;
          }
        }
    group: root
    mode: "000644"
    owner: root
